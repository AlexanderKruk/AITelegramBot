{
    "env": {
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": ["airbnb-base",
                "prettier",
                 "plugin:node/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "error",
        "no-console": "off",
    }
}